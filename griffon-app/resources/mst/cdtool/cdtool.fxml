<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>


<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mst.cdtool.CdtoolController">
   <children>
      <Accordion maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
        <panes>
            <TitledPane text="Counter">
               <content>
                  <GridPane prefHeight="60.0" prefWidth="200.0">
                      <children>
                          <Label fx:id="clickLabel" text="0" />
                          <Button fx:id="clickActionTarget" mnemonicParsing="false" prefWidth="200.0" text="Click" GridPane.rowIndex="1" />
                      </children>
                     <columnConstraints>
                        <ColumnConstraints />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints />
                        <RowConstraints />
                     </rowConstraints>
                  </GridPane>
               </content>
            </TitledPane>
            <TitledPane animated="false" text="Music Database">
              <content>
                <AnchorPane id="graphPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </TitledPane>
        </panes>
      </Accordion>
   </children>
</StackPane>
